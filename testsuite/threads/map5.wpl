fun f1(x : arr[3] bit) {
   var y : int := 0;
   var z : arr [3] bit;
  
   for i in [0, 20] {
       y := int(x[0]) * i;
       y := int(x[1]) * y;
       y := y * int(x[2]);
       y := int(x[1]) + y + 1
   };
 
   -- y := 1;
   z[0] := x[0];
   z[1] := bit(y);
   z[2] := bit(y);
   return z;
}

let comp main = read >>> map f1 |>>>| map f1 >>> write[arr [3] bit]
